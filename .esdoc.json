{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "coverage": { "enable": false },
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "typeInference": { "enable": true },
        "accessor": {
          "access": ["public"],
          "autoPrivate": true
        },
        "manual": {
          "index": "./README.md",
          "asset": "",
          "globalIndex": true,
          "files": []
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": { "all": true }
    }
  ]
}
